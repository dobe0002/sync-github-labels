{
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "globals": {
    "styles": true
  },
  "env": {
    "jest": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-var": "error",
    "curly": "error",
    "one-var": "error",
    "no-console": "error",
    "no-underscore-dangle": "off"
  }
}
